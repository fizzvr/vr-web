<!doctype html>
<html lang="es">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title><%= @obtenerTitulo() %></title>
        <meta name="description" content="<%= @obtenerDescripcion() %>">
        <%- @getBlock('meta').toHTML() %>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="apple-touch-icon-precomposed" href="apple-touch-icon-precomposed.png">
        <link rel="shortcut icon" href="favicon.ico">
        <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
        <%- @getBlock('styles').add(@site.cvr1).toHTML() %>
        <script async defer id="github-bjs" src="https://buttons.github.io/buttons.js"></script>
        <script src="https://apis.google.com/js/platform.js" async defer>{lang: 'es'}</script>
    </head>
    <body id="page-top" data-spy="scroll" data-target=".navbar-custom">
        <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
        <%- @content %>
        <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?key=<%- @site.apiv3 %>&amp;sensor=false"></script>
        <%- @getBlock('scripts').add(@site.jvr1).toHTML() %>
	   <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
          ga('create', 'UA-<%= @site.anal %>', 'auto');
          ga('send', 'pageview');
        </script>
    </body>
</html>
